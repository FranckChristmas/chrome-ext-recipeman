{
  "manifest_version": 3,
  "name": "URL to Make",
  "version": "1.0",
  "description": "send the current website URL to Make via Webhook",
  "icons": {
    "128": "recipeman_icon.png"
  },
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_title": "Click to send URL"
  },
  "background": {
    "service_worker": "background.js"
  }
}
